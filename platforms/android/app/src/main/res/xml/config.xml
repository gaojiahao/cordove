<?xml version='1.0' encoding='utf-8'?>
<widget id="com.refordom.roletask" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="StatusBar">
        <param name="android-package" value="org.apache.cordova.statusbar.StatusBar" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileOpener2">
        <param name="android-package" value="io.github.pwlin.cordova.plugins.fileopener2.FileOpener2" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Badge">
        <param name="android-package" value="de.appplant.cordova.plugin.badge.Badge" />
    </feature>
    <name>路塔流程</name>
    <description>
        路塔流程，连接断点，业务单元流程端到端一体化
    </description>
    <author email="jeff.lin@roletask.com" href="http://refordom.com">
        深圳市瑞福登信息技术服务有限公司
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/logo-0522-500.png" />
    <icon density="mdpi" src="res/icon/android/logo-0522-500.png" />
    <icon density="hdpi" src="res/icon/android/logo-0522-500.png" />
    <icon density="xhdpi" src="res/icon/android/logo-0522-500.png" />
    <icon density="xxhdpi" src="res/icon/android/logo-0522-500.png" />
    <splash density="land-hdpi" src="res/splash/android/app-welcome.png" />
    <splash density="land-ldpi" src="res/splash/android/app-welcome.png" />
    <splash density="land-mdpi" src="res/splash/android/app-welcome.png" />
    <splash density="land-xhdpi" src="res/splash/android/app-welcome.png" />
    <splash density="port-hdpi" src="res/splash/android/app-welcome.png" />
    <splash density="port-ldpi" src="res/splash/android/app-welcome.png" />
    <splash density="port-mdpi" src="res/splash/android/app-welcome.png" />
    <splash density="port-xhdpi" src="res/splash/android/app-welcome.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="iosPersistentFileLocation" value="Compatibility" />
</widget>
